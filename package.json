{
  "name": "online-scoreboard",
  "version": "0.1.0",
  "engines": {
    "node": "10.x"
  },
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "now dev",
