    "react-avataaars": "^0.2.3",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^2.0.1",
    "@types/dotenv": "^6.1.1",
