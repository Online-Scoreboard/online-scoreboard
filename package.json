    "lint": "eslint --ext=ts,tsx src/**/*"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@reach/router": "^1.2.1",
    "apollo-boost": "^0.4.4",
    "aws-amplify": "^1.1.40",
    "dotenv": "^8.1.0",
    "graphql": "^14.5.8",
    "react": "^16.10.2",
    "react-avataaars": "^0.2.3",
    "react-dom": "^16.10.2",
    "react-scripts": "^3.2.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^2.0.3",
    "@types/dotenv": "^6.1.1",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.18",
    "@types/node": "^12.7.11",
